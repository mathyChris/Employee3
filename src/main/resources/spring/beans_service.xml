</bean>

<!-- StoreUnRegisterService -->
<bean id="storeUnRegisterService" class="com.hybrid.service.StoreUnRegisterService">
    <property name="storeDao" ref="storeDao"/>
</bean>

<!-- StoreloginService -->
<bean id="storeLoginService" class="com.hybrid.service.StoreLoginService">
    <property name="storeDao" ref="storeDao"/>
